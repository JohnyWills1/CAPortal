{
  "connection": "default",
  "collectionName": "forumusers",
  "info": {
    "name": "forumuser",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "Username": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "Password": {
      "type": "password",
      "required": true
    },
    "Email": {
      "type": "email",
      "required": true
    },
    "Karma": {
      "type": "integer"
    }
  }
}
