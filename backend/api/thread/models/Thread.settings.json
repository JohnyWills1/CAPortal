{
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "thread",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "posts": {
      "collection": "post"
    },
    "user": {
      "model": "user",
      "via": "threads",
      "plugin": "users-permissions"
    },
    "admin_accepted": {
      "type": "boolean"
    }
  }
}