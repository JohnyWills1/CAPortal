{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "tags": {
      "collection": "tag"
    },
    "comments": {
      "collection": "comments",
      "via": "post"
    },
    "user": {
      "model": "user",
      "via": "posts",
      "plugin": "users-permissions"
    },
    "like_count": {
      "type": "integer"
    }
  }
}